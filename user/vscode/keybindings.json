[
  // Cycling in lists can be done via Tab and Shift+Tab
  // Reset
  {
    "key": "tab",
    "command": "-acceptSelectedSuggestion",
    "when": "suggestWidgetHasFocusedSuggestion && suggestWidgetVisible && textInputFocus"
  },
  {
    "key": "ctrl+shift+b",
    "command": "-workbench.action.tasks.build",
    "when": "taskCommandsRegistered"
  },
  {
    "key": "ctrl+s",
    "command": "-workbench.action.files.save"
  },
  {
    "key": "ctrl+q",
    "command": "-workbench.action.quit"
  },
  {
    "key": "ctrl+shift+t",
    "command": "-workbench.action.reopenClosedEditor"
  },
  {
    "key": "ctrl+shift+w",
    "command": "-workbench.action.closeWindow"
  },
  {
    "key": "ctrl+t",
    "command": "-workbench.action.showAllSymbols"
  },
  {
    "command": "workbench.action.focusActiveEditorGroup",
    "key": "escape",
    "when": "sideBarFocus || panelFocus"
  },

  // Window Management
  {
    "key": "ctrl+b",
    "command": "workbench.action.togglePanel"
  },
  {
    "key": "alt+b",
    "command": "workbench.action.toggleMaximizedPanel"
  },
  {
    "key": "ctrl+shift+a",
    "command": "workbench.action.toggleActivityBarVisibility"
  },
  {
    "key": "ctrl+shift+e",
    "command": "workbench.view.explorer",
    "when": "viewContainer.workbench.view.explorer.enabled"
  },

  // Quick Open
  {
    "key": "ctrl ctrl",
    "command": "workbench.action.quickTextSearch"
  },
  {
    "key": "shift shift",
    "command": "workbench.action.quickOpen"
  },
  {
    "key": "ctrl+e",
    "command": "workbench.action.showAllEditorsByMostRecentlyUsed"
  },
  {
    "key": "ctrl+o",
    "command": "workbench.action.files.openFile"
  },
  {
    "key": "ctrl+shift+o",
    "command": "workbench.action.files.openFolder",
    "when": "openFolderWorkspaceSupport"
  },
  {
    "key": "alt+o",
    "command": "workbench.action.openRecent"
  },

  // Find
  {
    "key": "ctrl+shift+f ctrl+shift+f",
    "command": "workbench.action.findInFiles"
  },
  {
    "key": "ctrl+shift+f ctrl+shift+e",
    "command": "search.action.openNewEditor"
  },

  // Git
  {
    "key": "ctrl+shift+g ctrl+shift+g",
    "command": "workbench.view.scm",
    "when": "workbench.scm.active"
  },
  {
    "key": "ctrl+shift+g ctrl+shift+b",
    "command": "git.checkout"
  },

  // Testing
  {
    "key": "ctrl+shift+t ctrl+shift+t",
    "command": "workbench.view.extension.test"
  },
  {
    "key": "ctrl+shift+t ctrl+shift+r",
    "command": "testing.runAll"
  },
  {
    "key": "ctrl+shift+t ctrl+shift+c",
    "command": "testing.coverageAll"
  },

  // Debug & Tasks
  {
    "key": "ctrl+shift+d ctrl+shift+d",
    "command": "workbench.view.debug",
    "when": "viewContainer.workbench.view.debug.enabled"
  },
  {
    "key": "ctrl+shift+d ctrl+shift+t",
    "command": "workbench.action.tasks.runTask"
  },
  {
    "key": "ctrl+shift+d ctrl+shift+r",
    "command": "workbench.action.tasks.restartTask"
  },
  {
    "key": "ctrl+shift+d ctrl+shift+c",
    "command": "workbench.action.debug.configure"
  },

  // Terminal
  {
    "key": "ctrl+\\",
    "command": "workbench.action.terminal.split",
    "when": "terminalFocus && terminalProcessSupported || terminalFocus && terminalWebExtensionContributedProfile"
  },
  {
    "key": "alt+\\",
    "command": "workbench.action.terminal.new",
    "when": "terminalFocus && terminalProcessSupported || terminalFocus && terminalWebExtensionContributedProfile"
  },
  {
    "key": "ctrl+w",
    "command": "workbench.action.terminal.kill",
    "when": "terminalFocus && terminalProcessSupported || terminalFocus && terminalWebExtensionContributedProfile"
  },

  // AI
  {
    "key": "ctrl+i",
    "command": "inlineChat.start",
    "when": "editorFocus && inlineChatHasEditsAgent && inlineChatPossible && !editorReadonly && !editorSimpleInput || editorFocus && inlineChatHasProvider && inlineChatPossible && !editorReadonly && !editorSimpleInput"
  },
  {
    "key": "ctrl+shift+i",
    "command": "workbench.action.chat.openInNewWindow" // Can be closed with Ctrl + W
  },

  // Editor
  {
    "key": "ctrl+a",
    "command": "editor.action.selectAll"
  },
  {
    "key": "ctrl+g",
    "command": "workbench.action.gotoLine"
  },
  {
    "key": "ctrl+s",
    "command": "workbench.action.files.saveFiles"
  },
  {
    "key": "ctrl+shift+s",
    "command": "workbench.action.files.saveAs"
  },
  {
    "key": "ctrl+w",
    "command": "workbench.action.closeActiveEditor",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+w",
    "command": "workbench.action.closeOtherEditors",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+w",
    "command": "workbench.action.closeEditorsInGroup",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+t",
    "command": "workbench.action.reopenClosedEditor",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+\\",
    "command": "workbench.action.splitEditor",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+\\",
    "command": "workbench.action.splitEditorDown",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+f",
    "command": "actions.find",
    "when": "editorFocus || editorIsOpen"
  },
  {
    "key": "ctrl+h",
    "command": "editor.action.startFindReplaceAction",
    "when": "editorFocus || editorIsOpen"
  },
  {
    "key": "ctrl+j",
    "command": "editor.action.joinLines",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+r",
    "command": "editor.action.smartSelect.expand",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+r",
    "command": "editor.action.smartSelect.shrink",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+up",
    "command": "editor.action.moveLinesUpAction",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "alt+down",
    "command": "editor.action.moveLinesDownAction",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+`",
    "command": "editor.action.transformToUppercase",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+`",
    "command": "editor.action.transformToLowercase",
    "when": "editorTextFocus"
  },
  {
    "key": "f3",
    "command": "editor.action.showHover",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+f3",
    "command": "errorLens.copyProblemMessage",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+enter",
    "command": "editor.action.quickFix",
    "when": "editorHasCodeActionsProvider && textInputFocus && !editorReadonly"
  },
  {
    "key": "ctrl+enter",
    "command": "editor.action.insertLineAfter",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+shift+enter",
    "command": "editor.action.insertLineBefore",
    "when": "editorTextFocus && !editorReadonly"
  },

  // Multi Cursor Handling -> can be removed with Escape
  {
    "key": "ctrl+u",
    "command": "cursorUndo",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+shift+up",
    "command": "editor.action.insertCursorAbove",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+down",
    "command": "editor.action.insertCursorBelow",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+d",
    "command": "notebook.addFindMatchToSelection",
    "when": "config.notebook.multiCursor.enabled && notebookCellEditorFocused && activeEditor == 'workbench.editor.notebook'"
  },
  {
    "key": "ctrl+shift+d",
    "command": "editor.action.addSelectionToPreviousFindMatch",
    "when": "textInputFocus"
  },
  {
    "key": "alt+d",
    "command": "editor.action.insertCursorAtEndOfEachLineSelected",
    "when": "textInputFocus"
  },

  // Tab Handling
  {
    "key": "tab",
    "command": "selectNextSuggestion",
    "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"
  },
  {
    "key": "shift+tab",
    "command": "selectPrevSuggestion",
    "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"
  },
  {
    "key": "tab",
    "command": "workbench.action.quickOpenSelectNext",
    "when": "inQuickOpen"
  },
  {
    "key": "shift+tab",
    "command": "workbench.action.quickOpenSelectPrevious",
    "when": "inQuickOpen"
  },

  // Editor Group Movement
  {
    "key": "alt+1",
    "command": "workbench.action.focusFirstEditorGroup"
  },
  {
    "key": "alt+2",
    "command": "workbench.action.focusSecondEditorGroup"
  },
  {
    "key": "alt+3",
    "command": "workbench.action.focusThirdEditorGroup"
  },
  {
    "key": "alt+4",
    "command": "workbench.action.focusFourthEditorGroup"
  },
  {
    "key": "alt+5",
    "command": "workbench.action.focusFifthEditorGroup"
  },
  {
    "key": "alt+6",
    "command": "workbench.action.focusSixthEditorGroup"
  },
  {
    "key": "alt+7",
    "command": "workbench.action.focusSeventhEditorGroup"
  },
  {
    "key": "alt+8",
    "command": "workbench.action.focusEighthEditorGroup"
  },
  {
    "key": "alt+9",
    "command": "workbench.action.focusNinthEditorGroup"
  }
]
